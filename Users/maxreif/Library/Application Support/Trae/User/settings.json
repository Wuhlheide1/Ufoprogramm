{
  "redhat.telemetry.enabled": false,
  "workbench.colorTheme": "Deep Blue",
  "git.autofetch": true,
  "editor.formatOnSave": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "'Monaspace Argon Var','JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontLigatures.featureSettings": "\"calt\" on, \"liga\" on, \"dlig\" on, \"ss01\", \"ss02\", \"ss03\", \"ss04\", \"ss05\", \"ss06\", \"ss07\", \"ss08\", \"ss09\",",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "ghost-text",
        "settings": {
          "fontStyle": "italic",
          "fontFamily": "Monaspace Radon Var"
        }
      }
    ]
  }
}